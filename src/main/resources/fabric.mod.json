{
	"schemaVersion": 1,
	"id": "fpsdisplay",
	"version": "${version}",
	"name": "FPSDisplay",
	"description": "A simple mod that displays the current FPS on the screen.",
	"authors": [
		"HoneyBerries"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/fpsdisplay/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.honeyberries.FPSDisplay"
		],
		"client": [
			"net.honeyberries.FPSDisplayClient"
		]
	},
	"mixins": [
		"fpsdisplay.mixins.json",
		{
			"config": "fpsdisplay.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}