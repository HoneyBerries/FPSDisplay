{
	"schemaVersion": 1,
	"id": "fpsdisplay",
	"version": "${version}",
	"name": "FPSDisplay",
	"description": "A simple mod that displays the current FPS on the screen.",
	"authors": [
		"HoneyBerries"
	],
	"contact": {
		"homepage": "https://honeyberries.net/",
		"sources": "https://github.com/HoneyBerries/FPSDisplay"
	},
	"license": "GPL-3.0",
	"icon": "assets/fpsdisplay/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"net.honeyberries.FPSDisplay"
		],
		"client": [
			"net.honeyberries.FPSDisplayClient"
		],
		"modmenu": [
			"net.honeyberries.FPSDisplayModMenu"
		]
	},
	"mixins": [
		"fpsdisplay.mixins.json",
		{
			"config": "fpsdisplay.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*",
		"modmenu": "*"
	},

	"custom": {
		"modmenu": {
			"links": {
			  	"modmenu.discord": "https://discord.gg/TbkHCshxjS"
			},
			"update_checker": true
		}
	}
}